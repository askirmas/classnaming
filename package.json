{
  "name": "react-classnaming",
  "version": "0.2.0",
  "description": "Easy applied function to use with CSS modules and do not get lost in CSS classes",
  "main": "dist",
  "scripts": {
    "dev": "npm run test -- --watch --coverage=false",
    "build": "npm run compile",
    "test": "npm run build",
    "prebuild": "npm run unit",
    "postbuild": "npm run spec",
    "release": "np",
    "prerelease": "npm run build",
    "postrelease": "npm update",
    "unit": "npm run jest",
    "spec": "cd __recipes__ && npm run test",
    "compile": "rm -rf dist && tsc --project tsconfig.compile.json",
    "setup": "git config include.path ../.gitconfig && git-hooks-wrapper init",
    "jest": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askirmas/react-classnaming.git"
  },
  "keywords": [
    "react",
    "className",
    "classNames",
    "css",
    "modules"
  ],
  "author": "Andrii Kirmas <kirmas@phystech.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/askirmas/react-classnaming/issues#bugs"
  },
  "homepage": "https://github.com/askirmas/react-classnaming#readme",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@types/react": ">=15"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "git-hooks-wrapper": "^0.7.1",
    "jest": "^26.6.3",
    "np": "^7.3.0",
    "react": "^17.0.1",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "np": {
    "yarn": false
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ]
}
